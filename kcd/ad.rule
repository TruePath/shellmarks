#
# For autodep
#

OUTPUT	Makefile.in

DEFTARGETLIST	default-target
SUBDIR	po

# config.h is generated by configure and is not in $(srcdir)
CURDIR	config.h

# Do not pickup generated curses.h for make rules
EXCLUDE curses.h

# The \" is for C++ string literal
# The " is to prevent shell from interpreting inside variable
TARGET	kcdmain		C++
	buffer.cc
	conffile.cc -DKCD_ETCDIR=\""$(etcdir)"\"
	confobj.cc
	cstrlib.cc
	dirobj.cc
	dirtree.cc
	dirutil.cc
	editbox.cc
	error.cc
	gentree.cc
	gtstream.cc
	gzfileio.cc
	kcdmain.cc -DKCD_LOCALEDIR=\""$(localedir)"\"
	khdoc.cc
	khwin.cc
	miscobj.cc
	scandir.cc
	scrnman.cc
	scroll.cc
	seek.cc
	statwin.cc
	strmisc.cc
	utf8dec.cc
